        "def month_all(n): # 강남/강서구청의 n번째 달 출력 함수(n=1~12)\n",
        "  a = 0\n",
        "  b = 0\n",
        "  for i in range(len(df)):\n",
        "    for j in range(1,n+1):\n",
        "      if int(df['대여일시'][i].split('-')[1]) == j:\n",
        "        df_n = []\n",
        "        df_n.append(df['대여일시'][i])\n",
        "        a += len(df_n)\n",
        "  if n>1:\n",
        "    for k in range(len(df)):\n",
        "      for m in range(1,n):\n",
        "        if int(df['대여일시'][k].split('-')[1]) == m:\n",
        "          df_n = []\n",
        "          df_n.append(df['대여일시'][k])\n",
        "          b += len(df_n)\n",
        "  df_n = df[b:a]\n",
        "  return df_n"
      ],
